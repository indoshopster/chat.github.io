<script type="text/javascript" src="https://code.jquery.com/jquery-3.3.1.min.js"></script>
                                            
<script type="text/javascript">
    $(document).ready(function(){console.log("ready!");var btn_el=$('input[type="submit"] button[type="submit"]');var form=$("form")form.submit(function(event){onSubmit();event.preventDefault()})function onSubmit(){var input_el=$("input");var select_el=$("select");var textarea_el=$("textarea");var dynamicData={};if(input_el.length>0){for(var i=0;i<=input_el.length;i++){if(typeof input_el[i]!=='undefined'&&input_el[i].type!=="submit"){if(input_el[i].type==="radio"){if(input_el[i].checked){dynamicData[input_el[i].name]=input_el[i].value}}else if(input_el[i].type==="checkbox"){if(input_el[i].checked){if(input_el[i].name in dynamicData){dynamicData[input_el[i].name]+=', '+input_el[i].value}else{dynamicData[input_el[i].name]=input_el[i].value}}}else{dynamicData[input_el[i].name]=input_el[i].value}}}}if(select_el.length>0){for(var i=0;i<=select_el.length;i++){if(typeof select_el[i]!=='undefined'){dynamicData[select_el[i].name]=select_el[i].value}}}if(textarea_el.length>0){for(var i=0;i<=textarea_el.length;i++){if(typeof textarea_el[i]!=='undefined'){dynamicData[textarea_el[i].name]=textarea_el[i].value}}}console.table("dynamicDatass",dynamicData);fetch("http://localhost:5000/notification/project/5c6d217645a0790dab1d1369",{method:"POST",body:JSON.stringify(dynamicData),headers:{'Content-Type':'application/json'},}).then(response=>response.json()).then(data=>console.log("msg",data))}})
</script>
